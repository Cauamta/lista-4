#include <stdio.h>
#include <stdlib.h>
/*
Aluno:cauã mata
Matricula:2412130053
*/

int main() {
    int vetor[8];
    int X, Y;

    for (int i = 0; i < 8; i++) {
        printf("Digite o valor para a posição %d: ", i);
        scanf("%d", &vetor[i]);
    }

    printf("Digite o valor de X e Y: ");
    scanf("%d %d", &X, &Y);

    int soma = vetor[X] + vetor[Y];

    printf("A soma dos valores nas posições X e Y é: %d\n", soma);

    return 0;
}
